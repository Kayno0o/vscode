{
  "publisher": "kayno0o",
  "name": "kvsc",
  "displayName": "KVSCode",
  "version": "0.0.1",
  "description": "Extension to create query, provider, etc. for Symfony projects",
  "author": {
    "name": "Kayno0o",
    "url": "https://github.com/kayno0o"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kayno0o/vscode"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "engines": {
    "vscode": "^1.99.1"
  },
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "kvsc.processor",
        "title": "KVSC: Processor"
      },
      {
        "command": "kvsc.provider",
        "title": "KVSC: Provider"
      },
      {
        "command": "kvsc.repository",
        "title": "KVSC: Repository"
      },
      {
        "command": "kvsc.entity",
        "title": "KVSC: Entity"
      },
      {
        "command": "kvsc.toggleQuotes",
        "title": "KVSC: Single to Double Quote"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "kvsc.repository",
          "when": "explorerResourceIsFolder && resourcePath =~ /[\\/]src[\\/]Repository$/",
          "group": "navigation"
        },
        {
          "command": "kvsc.entity",
          "when": "explorerResourceIsFolder && resourcePath =~ /[\\/]src[\\/]Entity$/",
          "group": "navigation"
        },
        {
          "command": "kvsc.provider",
          "when": "explorerResourceIsFolder && resourcePath =~ /[\\/]src[\\/]State$/",
          "group": "navigation"
        },
        {
          "command": "kvsc.processor",
          "when": "explorerResourceIsFolder && resourcePath =~ /[\\/]src[\\/]State$/",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "key": "ctrl+alt+'",
        "command": "kvsc.toggleQuotes",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "nr compile",
    "compile": "bun x tsup",
    "pretest": "nr compile && nr lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@kaynooo/utils": "^1.2.17"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.14.1",
    "@kaynooo/eslint": "^1.1.7",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.0.3",
    "@types/vscode": "^1.101.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.29.0",
    "eslint-plugin-github": "^6.0.0",
    "tsup": "^8.5.0",
    "vscode": "^1.1.37"
  }
}
